#include<stdio.h>
#include<ctype.h>
int main(){
	int i,j,x,y;
	char ch='*';
	printf("__________________________________________________\n");
	printf("Programa que imprime um QUADRILATERO em forma de '%c'\n", ch);
	printf("__________________________________________________\n");
	printf("De o numero de linhas: ");
	scanf("%i", &x);
	printf("De o numero de colunas: ");
	scanf("%i", &y);
	printf("\n");
	for(i=1;i<=x;i++){
		for(j=1;j<=y;j++){
			printf(" %c", ch);
		}
		printf("\n");
	}
}
